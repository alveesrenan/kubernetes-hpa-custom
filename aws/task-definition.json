{
  "family": "service-kubernetes-hpa-custom",
  "taskRoleArn": "arn:aws:iam::503622861177:role/ECSTaskRoleForAcars",
  "containerDefinitions": [
    {
      "name": "decoder-acars",
      "image": "503622861177.dkr.ecr.sa-east-1.amazonaws.com/decoder-acars",
      "cpu": 0,
      "memory": 384,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "kubernetes-hpa-custom-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "kubernetes-hpa-custom-logs"
        }
      }
      "environment": [
        {
          "name": "DISCOVERY_PORT",
          "value": "8500"
        },
        {
          "name": "PROFILE",
          "value": "prod"
        },
        {
          "name": "MESSAGING_QUEUE_NAME",
          "value": "decoder-acars-processor"
        },
        {
          "name": "LDI_URL",
          "value": "http://ldi-service"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "123456"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://ahpapidb.csno4zzuco71.sa-east-1.rds.amazonaws.com/decoder_acars"
        },
        {
          "name": "MESSAGING_QUEUE_HOSTNAME",
          "value": "https://sqs.sa-east-1.amazonaws.com/503622861177"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "decoder_acars"
        },
        {
          "name": "BUCKETEER_BUCKET_NAME",
          "value": "aheadpro-acars-file"
        },
        {
          "name": "DISCOVERY_HOST",
          "value": "http://internal-ahp-internal-lb-20148387.sa-east-1.elb.amazonaws.com"
        },
        {
          "name": "JAVA_ENV",
          "value": "prod"
        },
        {
          "name": "LOG_LEVEL",
          "value": "WARN"
        }
      ]
    }
  ]
}
